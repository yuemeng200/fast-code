{
  "name": "fast-code",
  "displayName": "fast-code",
  "description": "ðŸš€",
  "version": "0.3.3",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "static/img/logo.png",
  "publisher": "amonduul",
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:vue"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fast-code.enterComponentDefination",
        "title": "Enter component defination (Focus the component line -> Command + E)"
      },
      {
        "command": "fast-code.convertColor",
        "title": "Convert color (Focus the color line && Command + Shift + C)"
      }
    ],
    "keybindings": [
      {
        "command": "fast-code.enterComponentDefination",
        "key": "ctrl+e",
        "mac": "cmd+e",
        "when": "editorTextFocus && editorLangId == 'vue'"
      },
      {
        "command": "fast-code.convertColor",
        "key": "ctrl+shift+c",
        "mac": "cmd+shift+c",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "fast-code",
      "properties": {
        "fast-code.componentAutoRegistration": {
          "type": "boolean",
          "default": false,
          "description": "Enable automatic component registration"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "publish": "vsce publish --no-dependencies --allow-missing-repository"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "~18.19.21",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^2.24.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/generator": "^7.23.6",
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.5",
    "@vue/compiler-sfc": "^3.4.21",
    "fs-extra": "^11.2.0"
  }
}